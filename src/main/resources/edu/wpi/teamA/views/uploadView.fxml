<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teamA.views.UploadController">
   <children>
      <Button fx:id="selectFile" layoutX="230.0" layoutY="140.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="109.0" text="Select file" />
      <Text fx:id="displayTxt" layoutX="156.0" layoutY="124.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please upload your CSV file here">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Text fx:id="fileTxt" layoutX="190.0" layoutY="158.0" strokeType="OUTSIDE" strokeWidth="0.0" text="File: " wrappingWidth="33.3115234375" />
      <Button fx:id="confirmUpload" layoutX="234.0" layoutY="187.0" mnemonicParsing="false" onAction="#handleConfirmAction" text="Confirm upload" />
      <Button fx:id="canUpload" layoutX="116.0" layoutY="187.0" mnemonicParsing="false" text="Cancel upload" />
      <Button fx:id="returnButton" layoutX="355.0" layoutY="187.0" mnemonicParsing="false" onAction="#handleReturnAction" prefHeight="27.0" prefWidth="168.0" text="Return to previous page" />
      <Text fx:id="uploadTxt" layoutX="241.0" layoutY="124.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Uploaded">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
